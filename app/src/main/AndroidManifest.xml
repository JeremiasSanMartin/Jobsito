<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jobsito">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Jobsito">
        <activity
            android:name=".DescriptionActivity"
            android:exported="false" />
        <activity
            android:name=".ProfilePostuladosActivity"
            android:exported="false" />
        <activity
            android:name=".PostuladosActivity"
            android:exported="false" />
        <activity
            android:name=".profileViewActivity"
            android:exported="false" />
        <activity
            android:name=".publicEmActivity"
            android:exported="false" />
        <activity
            android:name=".InicioEmActivity"
            android:exported="false" />
        <activity
            android:name=".ModificarEmActivity"
            android:exported="false" />
        <activity
            android:name=".EmpresaActivity"
            android:exported="false" />
        <activity
            android:name=".InicioActivity"
            android:exported="false" />
        <activity
            android:name=".chooseActivity"
            android:exported="false" />
        <activity
            android:name=".ModificarActivity"
            android:exported="false" />
        <activity
            android:name=".PerfilPersona"
            android:exported="false" />
        <activity
            android:name=".InicioPersona"
            android:exported="false" />
        <activity
            android:name=".HomeActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name=".MyFirebaseMessagingService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>