<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ECEFF1"
    tools:context=".EmpresaActivity">


    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/textView2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/baloo"
            android:text="Email"
            android:textColor="@color/black"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/emailTextView"
            android:layout_width="match_parent"
            android:layout_height="25dp"
            android:fontFamily="@font/baloo"
            android:text="Email"
            android:textColor="@color/material_dynamic_neutral50"
            android:textSize="17dp" />

        <Space
            android:layout_width="match_parent"
            android:layout_height="8dp" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/baloo"
            android:text="Nombre de la empresa"
            android:textColor="@color/black"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/fullNameShowTextView"
            android:layout_width="match_parent"
            android:layout_height="25dp"
            android:fontFamily="@font/baloo"
            android:text="."
            android:textColor="@color/material_dynamic_neutral50"
            android:textSize="17dp" />

        <Space
            android:layout_width="match_parent"
            android:layout_height="8dp" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/baloo"
            android:text="TelÃ©fono"
            android:textColor="@color/black"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/phoneShowTextView"
            android:layout_width="match_parent"
            android:layout_height="25dp"
            android:fontFamily="@font/baloo"
            android:text="."
            android:textColor="@color/material_dynamic_neutral50"
            android:textSize="17dp" />

        <Space
            android:layout_width="match_parent"
            android:layout_height="8dp" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/baloo"
            android:text="Direccion"
            android:textColor="@color/black"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/direccionTextView"
            android:layout_width="match_parent"
            android:layout_height="25dp"
            android:fontFamily="@font/baloo"
            android:text="."
            android:textColor="@color/material_dynamic_neutral50"
            android:textSize="17dp" />

        <Space
            android:layout_width="match_parent"
            android:layout_height="8dp" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/baloo"
            android:text="CUIT"
            android:textColor="@color/black"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/cuitTextView"
            android:layout_width="match_parent"
            android:layout_height="25dp"
            android:fontFamily="@font/baloo"
            android:text="."
            android:textColor="@color/material_dynamic_neutral50"
            android:textSize="17dp" />

        <Space
            android:layout_width="match_parent"
            android:layout_height="8dp" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/baloo"
            android:text="Email de contacto"
            android:textColor="@color/black"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/contactEmailTextView"
            android:layout_width="match_parent"
            android:layout_height="25dp"
            android:fontFamily="@font/baloo"
            android:text="."
            android:textColor="@color/material_dynamic_neutral50"
            android:textSize="17dp" />

        <Space
            android:layout_width="match_parent"
            android:layout_height="8dp" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/baloo"
            android:text="Rubro de trabajo"
            android:textColor="@color/black"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/rubroShowTextView"
            android:layout_width="match_parent"
            android:layout_height="25dp"
            android:fontFamily="@font/baloo"
            android:text="."
            android:textColor="@color/material_dynamic_neutral50"
            android:textSize="17dp" />

        <Space
            android:layout_width="match_parent"
            android:layout_height="8dp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <Button
                android:id="@+id/modificarButton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@drawable/boton_redondo"
                android:fontFamily="@font/baloo"
                android:text="Editar perfil"
                android:textColor="@color/white"
                android:textSize="17dp" />

            <Space
                android:layout_width="8dp"
                android:layout_height="match_parent"
                android:layout_weight="0" />

            <Button
                android:id="@+id/logOutButton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@drawable/boton_redondo"
                android:fontFamily="@font/baloo"

                android:text="Cerrar sesion"
                android:textColor="#FFFFFF"
                android:textSize="17dp"
                tools:ignore="TextContrastCheck" /><![CDATA[







            />



        ]]>
        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="16dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <Space
            android:layout_width="match_parent"
            android:layout_height="10dp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <Button
                android:id="@+id/inicioButton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@drawable/boton_redondo"
                android:fontFamily="@font/baloo"
                android:text="Inicio"
                android:textColor="@color/white"
                android:textSize="17dp" />

            <com.google.android.material.floatingactionbutton.FloatingActionButton
                android:id="@+id/publicarButton"
                android:layout_width="wrap_content"
                android:layout_height="49dp"
                android:layout_gravity="end|bottom"
                android:layout_margin="3dp"
                android:contentDescription="@string/app_name"
                android:src="@drawable/ic_baseline_add_24"
                app:fabSize="auto"
                tools:ignore="MissingConstraints" />

            <Button
                android:id="@+id/perfilButton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@drawable/boton_redondo"
                android:fontFamily="@font/baloo"
                android:text="Perfil"
                android:textColor="@color/white"
                android:textSize="17dp" /><![CDATA[









            />




        ]]>
        </LinearLayout>
    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>